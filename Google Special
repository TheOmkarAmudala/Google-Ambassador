    <script>
        document.addEventListener('DOMContentLoaded', () => {

            const smoothScrollLinks = document.querySelectorAll('a[href^="#"]');
            for (const link of smoothScrollLinks) {
                link.addEventListener("click", (e) => {
                    e.preventDefault();
                    const href = link.getAttribute("href");
                    const targetElement = document.querySelector(href);
                    if(targetElement) {
                        targetElement.scrollIntoView({ behavior: "smooth" });
                    }
                });
            }

            const growthCtx = document.getElementById('growthChart').getContext('2d');
            new Chart(growthCtx, {
                type: 'bar',
                data: {
                    labels: ['Start', 'Current'],
                    datasets: [{
                        label: 'IEEE Members',
                        data: [10, 150],
                        backgroundColor: [
                            'rgba(168, 162, 158, 0.6)', // stone-400
                            'rgba(59, 130, 246, 0.6)' // blue-500
                        ],
                        borderColor: [
                            'rgba(120, 113, 108, 1)', // stone-500
                            'rgba(37, 99, 235, 1)' // blue-600
                        ],
                        borderWidth: 2
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        y: {
                            beginAtZero: true,
                            grid: {
                                color: 'rgba(214, 211, 209, 0.5)' // stone-300
                            }
                        },
                         x: {
                            grid: {
                                display: false
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            display: false
                        },
                        tooltip: {
                            callbacks: {
                                label: function(context) {
                                    let label = context.dataset.label || '';
                                    if (label) {
                                        label += ': ';
                                    }
                                    if (context.parsed.y !== null) {
                                        label += context.parsed.y + ' Members';
                                    }
                                    return label;
                                }
                            }
                        }
                    }
                }
            });

            const eventsCtx = document.getElementById('eventsChart').getContext('2d');
            new Chart(eventsCtx, {
                type: 'bar',
                data: {
                    labels: ['Event 1', 'Event 2', 'Event 3', 'Event 4', 'Event 5', 'Event 6', 'Event 7', 'Event 8+'],
                    datasets: [{
                        label: 'Attendees',
                        data: [120, 150, 110, 130, 160, 125, 140, 180],
                        backgroundColor: 'rgba(59, 130, 246, 0.6)',
                        borderColor: 'rgba(37, 99, 235, 1)',
                        borderWidth: 2
                    }]
                },
                options: {
                     responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        y: {
                            beginAtZero: true,
                            suggestedMin: 50,
                            suggestedMax: 200,
                             grid: {
                                color: 'rgba(214, 211, 209, 0.5)'
                            }
                        },
                         x: {
                            grid: {
                                display: false
                            }
                        }
                    },
                     plugins: {
                        legend: {
                            display: false
                        },
                         tooltip: {
                            callbacks: {
                                label: function(context) {
                                    return 'Attendees: >100';
                                }
                            }
                        }
                    }
                }
            });

            const toolData = {
                drive: { title: "Google Drive", desc: "Central to my organizational system for all project assets, documentation, and collaborative files." },
                docs: { title: "Google Docs", desc: "Used daily for drafting reports, planning event outlines, and creating shared technical documentation." },
                sheets: { title: "Google Sheets", desc: "My go-to for tracking project progress, managing member lists, and analyzing event feedback data." },
                meet: { title: "Google Meet", desc: "The primary tool for conducting team meetings, remote workshops, and coordinating with speakers." },
                calendar: { title: "Google Calendar", desc: "Essential for scheduling every event, meeting, and deadline, ensuring the entire team is synchronized." },
                firebase: { title: "Firebase", desc: "Leveraged for rapid prototyping and backend services in various web and hackathon projects." },
                gemini: { title: "Gemini", desc: "A key part of my workflow for brainstorming, code optimization, and exploring creative solutions to complex problems." }
            };

            const toolItems = document.querySelectorAll('.tool-item');
            const toolInfoTitle = document.querySelector('#tool-info .font-semibold');
            const toolDesc = document.getElementById('tool-desc');
            
            toolItems.forEach(item => {
                item.addEventListener('click', () => {
                    const toolKey = item.dataset.tool;
                    if (toolData[toolKey]) {
                        toolInfoTitle.textContent = toolData[toolKey].title;
                        toolDesc.textContent = toolData[toolKey].desc;
                    }

                    toolItems.forEach(i => i.classList.remove('ring-2', 'ring-blue-500'));
                    item.classList.add('ring-2', 'ring-blue-500');
                });
            });
        });
    </script>
